using BepInEx;
using HarmonyLib;
using UnityEngine;
using System;
using System.Collections.Generic;
using Eromancer;
using Eromancer.Animation;
using Eromancer.GameDB;
using Buttplug;
using Buttplug.Client.Connectors.WebsocketConnector;
using Buttplug.Client;

//deniszykov.WebSocketListener

public class ButtplugController{
    public string clientName = "Onnyx";
    public ButtplugClient client;
    public Uri uri;
    public ButtplugWebsocketConnector websocketConnector;

    public ButtplugController(){
        client = new ButtplugClient(clientName);
        uri = new Uri("wss://127.0.0.1:12345");
        websocketConnector = new ButtplugWebsocketConnector(uri);
       
        client.ConnectAsync(websocketConnector);

    }
}